<Ui>
  <Frame name="BaganatorCustomiseDialogCategoriesEditorTemplate" virtual="true" mixin="BaganatorCustomiseDialogCategoriesEditorMixin">
    <Size x="300" y="200"/>
    <Scripts>
      <OnLoad method="OnLoad"/>
      <OnHide method="OnHide"/>
    </Scripts>
    <Layers>
      <Layer level="BACKGROUND">
        <FontString inherits="GameFontHighlight" text="NAME">
          <Anchors>
            <Anchor point="TOPLEFT" x="15" y="-40"/>
          </Anchors>
        </FontString>
        <FontString inherits="GameFontHighlight" text="SEARCH">
          <Anchors>
            <Anchor point="TOPLEFT" x="15" y="-90"/>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <EditBox autoFocus="false" inherits="InputBoxTemplate" parentKey="CategoryName" letters="100">
        <Size y="30"/>
        <Anchors>
          <Anchor point="TOPLEFT" x="20" y="-55"/>
          <Anchor point="RIGHT" x="-10"/>
        </Anchors>
      </EditBox>
      <EditBox autoFocus="false" inherits="InputBoxTemplate" parentKey="CategorySearch" letters="100">
        <Size y="30"/>
        <Anchors>
          <Anchor point="TOPLEFT" x="20" y="-105"/>
          <Anchor point="RIGHT" x="-10"/>
        </Anchors>
      </EditBox>

      <Button parentKey="ApplyChangesButton" text="SAVE" inherits="UIPanelDynamicResizeButtonTemplate">
        <Anchors>
          <Anchor point="BOTTOMLEFT" x="15" y="0"/>
        </Anchors>
      </Button>

      <Button parentKey="DeleteButton" text="DELETE" inherits="UIPanelDynamicResizeButtonTemplate">
        <Anchors>
          <Anchor point="BOTTOMRIGHT" x="-15" y="0"/>
        </Anchors>
      </Button>
    </Frames>
  </Frame>
</Ui>
